{
  "expo": {
    "name": "ExhibitU",
    "slug": "ExhibitU",
    "version": "0.1.3a",
    "orientation": "portrait",
    "icon": "./assets/ExhibitU_icon.png",
    "splash": {
      "image": "./assets/ExhibitU_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.exhibitu.ios"
    },
    "web": {
      "favicon": "./assets/ExhibitU_icon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "svg", "svgx"]
    }
  }
}
